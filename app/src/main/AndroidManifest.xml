<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gym4u">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserRegister"></activity>
        <activity android:name=".UserLogin" />
        <activity
            android:name=".ui.login.InstructorLoginActivity"
            android:label="@string/title_activity_instructor_login"
            android:parentActivityName=".GettingStartedActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.gym4u.GettingStartedActivity" />
        </activity>
        <activity
            android:name=".ui.login.LoginActivityInstructor"
            android:label="@string/title_activity_login_instructor" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>